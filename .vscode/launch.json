{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Mocha Test",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"--timeout",
				"999999",
				"--colors",
				"${workspaceFolder}/test/index.js"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}